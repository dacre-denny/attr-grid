{
  "name": "css-grid",
  "version": "1.0.0",
  "description": "barebones, easy to use, css grid",
  "scripts": {
    "scss": "node-sass --source-map true --output-style compressed -o dist src/grid.scss",
    "build": "npm run scss",
    "watch": "onchange \"src\" -- npm run build"
  },
  "devDependencies": {
    "lodash": "^3.10.1",
    "onchange": "^3.2.1"
  }
}
